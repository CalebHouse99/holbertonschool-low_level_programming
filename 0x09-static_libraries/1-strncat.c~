#include <stdio.h>
#include "holberton.h"
/**
 * _strncat - dest combines with src
 * @dest: first string
 * @src: second string
 * @n: variable
 * Return: 0;
 */
char *_strncat(char *dest, char *src, int n)
{
int a = 0;
int b = 0;
while (dest[a] != '\0')
{
a++;
}
while (src[b] != '\0' && b < n)
{
dest[a] = src[b];
a++;
b++;
}
dest[a] = '\0';

return (dest);
}
